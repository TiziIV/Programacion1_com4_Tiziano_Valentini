# Ejercicio 1
total=float(input("Ingrese el total de la cuenta: "))
propina_minima=total*0.10
propina_sugerida=total*0.15
monto_total_min=propina_minima+total
monto_total_sug=propina_sugerida+total
print("El monto de la cuenta: ", total)
print("La propina minima es de: ", propina_minima)
print("La propina sugerida es de: ", propina_sugerida)
print("El monto total minimo es de: ", monto_total_min)
print("El monto total sugerido es de: ", monto_total_sug)
